{
    "name": "react-loadhandler",
    "version": "0.1.2",
    "private": false,
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.3"
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
